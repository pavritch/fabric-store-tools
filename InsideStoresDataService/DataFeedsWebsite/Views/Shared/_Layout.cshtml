<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
	<div class="page">
		<header>
			<div id="title">
                <div><img src="/images/insidestorelogo.gif" style="width:316px;height:76px;" /></div>
			</div>

		</header>
		<section id="main">
			@RenderBody()
		</section>
		<footer>
		</footer>
	</div>

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
