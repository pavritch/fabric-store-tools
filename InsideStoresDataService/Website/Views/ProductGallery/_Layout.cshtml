<!DOCTYPE html>
<html ng-app ="myApp">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />

        <title>@ViewBag.Title</title>

        @* fonts *@
        <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.1/css/font-awesome.min.css" rel="stylesheet">

        @* bootstrap and theme *@
        <link href="//netdna.bootstrapcdn.com/bootstrap/3.0.1/css/bootstrap.min.css" rel="stylesheet">
        <link href="//netdna.bootstrapcdn.com/bootswatch/3.0.0/cerulean/bootstrap.min.css" rel="stylesheet">
        <link href="//netdna.bootstrapcdn.com/bootswatch/3.0.0/cerulean/bootstrap.min.css" rel="stylesheet">
        <link rel="stylesheet" href="//yandex.st/highlightjs/7.4/styles/default.min.css">

        <link rel="stylesheet" href="~/Content/animate.min.css">
        @Styles.Render("~/Content/css")

        @RenderSection("head", required: false)

    </head>
    <body>
        <div class="container">
            <header>
                <div id="title">
                    <div><a href="/"><img src="~/Images/InsideStoresLogo.gif" style="width:316px;height:76px;" /></a></div>
                </div>
            </header>
            <section id="main" class="row">
                <div class="col-md-12">
                    @RenderBody()
                </div>
            </section>
            <footer></footer>
        </div>

        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
        <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.1/js/bootstrap.min.js"></script>
        <script src="/Scripts/bootbox.min.js"></script>
        <script src="//yandex.st/highlightjs/7.4/highlight.min.js"></script>


        @Scripts.Render("~/bundles/signalr")
        <script src="~/signalr/hubs"></script>

        <!-- Angular and Angular Animation -->
        <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.0/angular.min.js"></script>
        <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.0/angular-animate.min.js"></script>

        @RenderSection("scripts", required: false)
    </body>
</html>
